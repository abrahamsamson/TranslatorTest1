    <classpathentry kind="lib" path="C:/Users/HP/Downloads/auth-2.32.13.jar">
        <attributes>
            <attribute name="module" value="true"/>
        </attributes>
    </classpathentry>
    <classpathentry kind="lib" path="C:/Users/HP/Downloads/regions-2.32.13.jar">
        <attributes>
            <attribute name="module" value="true"/>
        </attributes>
    </classpathentry>

    <!-- Output directory -->
    <classpathentry kind="output" path="target/classes"/>
</classpath>
